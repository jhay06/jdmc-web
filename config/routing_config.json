{
    "url_rewrite":{
        "/":"controller/index.php",
        "/index.php":"controller/index.php",
        "/dashboard":"controller/dashboard.php",
        "/signup":"controller/index.php?action=signup",
        "/products":"controller/product.php",
        "/products/new":"controller/product-new.php",
        "/appointment":"controller/appointment.php",
        "/accreditation":"controller/accreditation.php",
        "/registration":"controller/registration.php",
        "/accreditation/new":"controller/accreditation-new.php",
        "/registration/new":"controller/registration-new.php",
        "/contact-us":"templates/global/contact-us.html",
        "/tutorials":"templates/global/tutorials.html",
        "/services":"controller/services.php",
        "/service/card":"templates/artifacts/services_card.html",
        "/logout":"controller/logout.php",
        "/about-us":"templates/global/about-us.html",
        "/staff/(.*)/edit":"controller/registration-new.php?action=edit&user=$1",
        "/dentist/(.*)/edit":"controller/accreditation-new.php?action=edit&user=$1",
        "/products/(.*)/edit":"controller/product-new.php?action=edit&product=$1",
        "/settings":"controller/settings.php",
        "/error/[0-9]":"templates/error/$1.html",
        "/forgot_password":"controller/forgot.php"
    },
    "error":{
        "404":"/error/404",
        "500":"/error/500"
    }

}